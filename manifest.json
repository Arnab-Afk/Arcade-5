{
    "manifest_version": 2,
    "name": "Parapage Summary Extension",
    "version": "1.0",
    "description": "Get Wikipedia summary for the current page by Arnab Bhowmik",
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "permissions": ["activeTab"]
  }